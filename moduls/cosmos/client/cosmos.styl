	.texture:before
		animation animatePlanetBack 20s linear infinite

.map-planet-marker
	outline none
	cursor pointer

	&.zoom-lt-7
		border-width 2px

	&.zoom-lt-5
		border-width 1px
		.texture:before
			display none
		.texture:after
			display none

	.texture
		position absolute
		top 3%
		left 3%
		width 94%
		height 94%
		border-radius 50%
		overflow hidden
		z-index 1

		&:before
			content ""
			position absolute
			display block
			top 75%
			left -20%
			width 400%
			height 100%
			background-size 50% 100%
			background-repeat repeat-x
			transform rotate(30deg)
			z-index -1
	
		&:after
			content ""
			background-size 100% 100%
			background-image url(/img/game/cosmomap/planet_shadow.png)
			position absolute
			display block
			top 0
			left 0
			width 100%
			height 100%
		
